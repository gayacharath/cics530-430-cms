<h1>Users</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th></th>
      <th>Full name</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
  	<td><input id="id" name="user[id]" value="",  type="checkbox", class = "check">
	<td><%=user.id%></td>
        <td><%= link_to user.full_name, user_path(user) %></td>
        <td><%= user.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_user_path(user), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', user_path(user), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>


