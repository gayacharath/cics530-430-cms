<h1>Users</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Full name</th>
      <th>Email</th>
      <th>Member Since</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.full_name, user_path(user) %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at.to_s(:slash_date) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


