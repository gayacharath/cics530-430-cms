<%= form_for @user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :full_name %><br />
    <%= f.text_field :full_name %>
  </p>
  <p>
    <%= f.label :pref_name %><br />
    <%= f.text_field :pref_name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :admin %><br />
    <%= f.check_box :admin %>
  </p>

  <p><%= f.submit %></p>
<% end %>
